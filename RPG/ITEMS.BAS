NEXT N
CLS
GOSUB load
DO
IF ITN < 1 THEN ITN = 200
IF ITN > 200 THEN ITN = 1
IF c < 1 THEN c = 1
IF c > 10 THEN c = 10
IF O(ITN, 0) < 1 THEN O(ITN, 0) = 16
IF O(ITN, 0) > 16 THEN O(ITN, 0) = 1
LOCATE 1, 1
COLOR 4: IF c = 1 THEN COLOR 12
PRINT "ITEM #"; ITN
COLOR 1: IF c = 2 THEN COLOR 9
PRINT "TYPE "; t$(O(ITN, 0), 0)
COLOR 7: IF c = 3 THEN COLOR 15
PRINT t$(O(ITN, 0), 1); O(ITN, 1)
COLOR 7: IF c = 4 THEN COLOR 15
PRINT t$(O(ITN, 0), 2); O(ITN, 2)
COLOR 7: IF c = 5 THEN COLOR 15
PRINT t$(O(ITN, 0), 3); O(ITN, 3)
COLOR 7: IF c = 6 THEN COLOR 15
PRINT t$(O(ITN, 0), 4); O(ITN, 4)
COLOR 7: IF c = 7 THEN COLOR 15
PRINT t$(O(ITN, 0), 5); O(ITN, 5)
COLOR 2: IF c = 8 THEN COLOR 10
PRINT "COST"; O(ITN, 6) * 10
COLOR 5: IF c = 9 THEN COLOR 13
IF O(ITN, 7) < 1 THEN O(ITN, 7) = 16
IF O(ITN, 7) > 16 THEN O(ITN, 7) = 1
PRINT "PICTURE "; ob$(O(ITN, 7))
COLOR 6: IF c = 10 THEN COLOR 14
PRINT "Name: "; O$(ITN); "                   "
d = 13
FOR ta = ITN - 5 TO ITN + 5
IF ta = ITN THEN GOTO skip
d = d + 1
ga = ta
IF ga < 1 THEN ga = ga + 200
IF ga > 200 THEN ga = ga - 200
gg = 0: IF c = 1 THEN gg = 1
IF a$ <> "4" AND a$ <> "6" THEN gg = 0
IF gg = 1 THEN
LOCATE d, 1
COLOR 14: PRINT "#"; ga; "    ";
LOCATE d, 10
COLOR 15: PRINT LEFT$(O$(ga) + "              ", 16);
LOCATE d, 27
COLOR 12: PRINT t$(O(ga, 0), 0); "        ";
LOCATE d, 60
COLOR 10: PRINT "Cost"; O(ga, 6) * 10; "        ";
END IF
IF a$ = "8" OR a$ = "2" OR gg = 1 THEN
LOCATE d, 40
COLOR 13
h = c - 2
IF h < 1 THEN h = 1
IF h > 5 THEN h = 5
PRINT t$(O(ga, 0), h); O(ga, h)
END IF
skip:
NEXT ta
IF O(ITN, 7) < 1 THEN O(ITN, 7) = 16
IF O(ITN, 7) > 16 THEN O(ITN, 7) = 1
p = O(ITN, 7)
DO: a$ = INKEY$: LOOP WHILE a$ = ""
IF a$ = "t" THEN GOSUB save: GOSUB text
IF a$ = "s" THEN GOSUB save
IF a$ = "l" THEN GOSUB load
IF a$ = "8" THEN c = c - 1
IF a$ = "2" THEN c = c + 1
IF c > 1 AND c < 10 THEN
IF a$ = "4" THEN O(ITN, c - 2) = O(ITN, c - 2) - 1
IF a$ = "6" THEN O(ITN, c - 2) = O(ITN, c - 2) + 1
IF a$ = "," THEN O(ITN, c - 2) = O(ITN, c - 2) - 100
IF a$ = "." THEN O(ITN, c - 2) = O(ITN, c - 2) + 100
IF a$ = " " THEN LOCATE c, 40: INPUT O(ITN, c - 2): LOCATE c, 40: PRINT "                ";
END IF
IF c = 1 THEN
IF a$ = "4" THEN ITN = ITN - 1
IF a$ = "6" THEN ITN = ITN + 1
END IF
IF c = 10 THEN
IF a$ = " " THEN LOCATE 10, 7: INPUT O$(ITN)
END IF
LOOP
save:
OPEN "Items.lst" FOR OUTPUT AS #1
FOR a = 1 TO 200
PRINT #1, O$(a)
FOR b = 0 TO 7
PRINT #1, O(a, b)
NEXT b
NEXT a
CLOSE #1
RETURN
text:
OPEN "Items.txt" FOR OUTPUT AS #1
FOR a = 1 TO 200 STEP 2
PRINT #1, "Item #"; a, , "Item #"; a + 1
PRINT #1, "Name: "; O$(a), "Name: "; O$(a + 1)
FOR b = 0 TO 5
PRINT #1, t$(O(a, 0), b); O(a, b), t$(O(a + 1, 0), b); O(a + 1, b)
NEXT b
PRINT #1, "Cost:"; O(a, 6), , "Cost:"; O(a + 1, 6)
PRINT #1, "Pic#"; O(a, 7), , "Pic#"; O(a + 1, 7)
PRINT #1, "****************************************************************"
NEXT a
CLOSE #1
RETURN
load:
OPEN "Items.lst" FOR INPUT AS #1
FOR a = 1 TO 200
INPUT #1, O$(a)
FOR b = 0 TO 7
INPUT #1, O(a, b)
NEXT b
NEXT a
CLOSE #1
RETURN
